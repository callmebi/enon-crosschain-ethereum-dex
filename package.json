    "drizzle": "1.4.0",
    "drizzle-react": "1.3.0",
    "eth-gas-reporter": "0.2.4",
    "ethereum-blockies-base64": "1.0.2",
    "ganache-cli": "6.4.5",
    "ipfs-http-client": "33.1.0",
    "jquery": "3.5.0",
    "node-sass": "7.0.0",
    "openzeppelin-solidity": "2.3.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-redux": "7.1.0",
